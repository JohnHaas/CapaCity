<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.capacityprototype"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.capacityprototype.FullscreenActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/app_name"
            android:theme="@style/FullscreenTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.capacityprototype.HomePage"
            android:label="@string/title_activity_home_page" >
        </activity>
        <activity
            android:name="com.example.capacityprototype.RecentSearchPage"
            android:label="@string/title_activity_recent_search_page" >
        </activity>
        <activity
            android:name="com.example.capacityprototype.SearchPage"
            android:label="@string/title_activity_search_page" >
        </activity>
        <activity
            android:name="com.example.capacityprototype.SearchLocationPage"
            android:label="@string/title_activity_search_location_page" >
        </activity>
    </application>

</manifest>
